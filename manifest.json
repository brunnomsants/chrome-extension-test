{
  "manifest_version": 3,
  "name": "interceptor",
  "version": "2.1",
  "description": "Intercept, Forward, Re-send, and Copy as cURL",
  "permissions": ["debugger", "storage", "notifications"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
