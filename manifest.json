{
  "manifest_version": 3,
  "name": "interceptor",
  "version": "2.1",
  "description": "Intercept, Forward, Re-send, and Copy as cURL",
  "permissions": ["debugger", "storage", "notifications"],
  "host_permissions": ["<all_urls>"],
  "icons": {},
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
